{
  "name": "frontend-project",
  "version": "0.1.0-alpha",
  "description": "Projeto desenvolvido como requisito para a formação em Análise e Desenvolvimento de Sistemas - Tema: Front-end",
  "keywords": [
    "Web",
    "Front-end",
    "NOGs",
    "ONGs",
    "Dev"
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Yuri Machado Luz",
  "type": "commonjs",
  "main": "index.html",
  "scripts": {
    "test": "jest",
    "start": "node server.js",
    "combine-css": "node scripts/combine-css.js",
    "build:css": "npm run combine-css && postcss src/css/combined.css -o src/css/main.min.css --config config/postcss.config.js",
    "lint": "eslint src/js/ --fix --config config/eslint.config.js",
    "format": "prettier --write src/js/**/*.js"
  },
  "dependencies": {
    "express": "^5.1.0",
    "modern-normalize": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "clean-css-cli": "^5.6.3",
    "cssnano": "^7.1.1",
    "eslint": "^9.38.0",
    "jest": "^30.2.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-nesting": "^13.0.2",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yuri-Machado-Luz/frontend-project.git"
  },
  "bugs": {
    "url": "https://github.com/Yuri-Machado-Luz/frontend-project/issues"
  },
  "homepage": "https://github.com/Yuri-Machado-Luz/frontend-project#readme"
}
